#include <iostream>
#include <string>
using namespace std;

class Decoder {
    string code;
    
    public:
    Decoder(string s) {
        code = s;
    }
    
    string get_decode() {
        string result = "";
        
        for (int i = 0; i < code.length(); i++) {
            char current = code[i];
            if (i + 1 < code.length() && code[i+1] >= '1' && code[i + 1] <= '9') {
                int count = code[i + 1] - '0';
                for (int j = 0; j < count; j++) {
                    result += current;
                }
                i++;
            } else {
                result += current;
            }
        }
        return result;
    }
};

int main() {
    Decoder d("A3BB2CCC2");
    cout << d.get_decode();
    return 0;
}

